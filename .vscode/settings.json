{
    // eslint 开启自动修复
    "editor.codeActionsOnSave": {
        "source.fixAll": false,
        "source.fixAll.eslint": false,
        "source.fixAll.stylelint": false
    },
    // 是否自动格式化
    "editor.formatOnSave": true,
    "prettier.enable": true,
    "stylelint.validate": [
        "css",
        "scss",
        "vue"
    ],
    "vue.codeActions.enabled": false
}