{
	"name": "project-admin",
	"private": true,
	"version": "1.2.7",
	"type": "module",
	"resolutions": {
		"bin-wrapper": "npm:bin-wrapper-china"
	},
	"scripts": {
		"dev": "npx simple-git-hooks & vite",
		"build": "vite build",
		"preview": "vite preview",
		"svgo": "svgo -f src/assets/icons",
		"lint": "npm-run-all -s lint:eslint lint:stylelint lint:prettier",
		"lint:eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --cache --fix",
		"lint:stylelint": "stylelint src/**/*.{css,sass,scss,vue} --cache --fix",
		"lint:prettier": "prettier --write \"./**/*.{html,css,js,cjs,vue,.sass,json,md}\"",
		"lint:lint-staged": "lint-staged",
		"postinstall": "simple-git-hooks",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.1.0",
		"@kangc/v-md-editor": "^2.3.15",
		"@vueuse/core": "^10.3.0",
		"@vueuse/integrations": "^10.3.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"@wcjiang/notify": "^2.1.0",
		"axios": "^1.4.0",
		"dayjs": "^1.11.9",
		"echarts": "^5.4.3",
		"element-plus": "^2.3.8",
		"hotkeys-js": "^3.11.2",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.1",
		"nprogress": "^0.2.0",
		"path-browserify": "^1.0.1",
		"path-to-regexp": "^6.2.1",
		"pinia": "^2.1.6",
		"pinia-plugin-persist": "^1.0.0",
		"print-js": "^1.6.0",
		"qrcode": "^1.5.3",
		"qs": "^6.11.2",
		"signature_pad": "^4.1.6",
		"splitpanes": "^3.1.5",
		"vue": "^3.3.4",
		"vue-cropperjs": "^5.0.0",
		"vue-router": "^4.2.4",
		"vue3-count-to": "^1.1.2"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.2.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-vue": "^9.16.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"npm-run-all": "^4.1.5",
		"postcss-html": "^1.5.0",
		"prettier": "^3.0.0",
		"sass": "^1.64.2",
		"simple-git-hooks": "^2.9.0",
		"stylelint": "^15.10.2",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-config-standard-scss": "^10.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"stylelint-scss": "^5.0.1",
		"svgo": "^3.0.2",
		"tailwindcss": "^3.3.3",
		"unplugin-auto-import": "^0.16.6",
		"unplugin-vue-components": "^0.25.1",
		"vite": "^4.4.8",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-style-import": "^2.0.0",
		"vite-plugin-stylelint": "^4.3.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-vue-inspector": "^3.5.0",
		"vite-plugin-vue-setup-extend": "^0.4.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
			"prettier --write--parser json"
		],
		"package.json": [
			"prettier --write"
		],
		"*.vue": [
			"eslint --fix",
			"prettier --write",
			"stylelint --fix"
		],
		"*.{scss,less,styl,html}": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.md": [
			"prettier --write"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged",
		"preserveUnused": true
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"postcss",
				"rollup",
				"react"
			]
		}
	}
}