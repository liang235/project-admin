		"@vueuse/integrations": "^10.9.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"@wcjiang/notify": "^2.1.3",
		"axios": "^1.6.8",
		"dayjs": "^1.11.10",
		"echarts": "^5.5.0",
		"element-plus": "^2.6.3",
		"focus-trap": "^7.5.4",
		"hotkeys-js": "^3.13.7",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.1",
		"nprogress": "^0.2.0",
